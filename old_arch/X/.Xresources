urxvt.scrollBar: false
!urxvt.transparent: true
!urxvt.shading: 20

!urxvt.tintColor: #172A47
!urxvt.inheritPixmap: true
urxvt*depth: 35
urxvt*background: rgba:0022/0064/0116/0000

urxvt.keysym.Control-Shift-V: perl:clipboard:paste
urxvt.keysym.Control-Shift-C: perl:clipboard:copy

!urxvt.iso14755: False

!urxvt.perl-ext-common: default,clipboard

!Fonts

urxvt.font: xft:Fira Code:pixelsize=11,style=regular
urxvt.boldFont: xft:Fira Code:pixelsize=11,style=bold
urxvt.letterSpace: -1


!Xft.dpi: 96	
Xft.antialias: true
!Xft.rbga: none
!Xft.hintstyle: hintfull
!Xft.hinting: true

*.internalBorder: 13


